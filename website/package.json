    "classnames": "^2.5.1",
    "clsx": "^2.0.0",
    "docusaurus-plugin-openapi-docs": "^4.5.1",
    "docusaurus-plugin-sass": "^0.2.6",
    "docusaurus-theme-openapi-docs": "^4.5.1",
    "lodash": "^4.17.21",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.0.0",
    "react-code-block": "^1.1.3",
    "react-dom": "^19.0.0",
    "react-hubspot-form": "^1.3.7",
    "sass": "^1.92.1"
